[<%= @pool['name'] %>]
    user = <%= @pool['user'] %>
    group = <%= @pool['group'] %>
    listen = <%= @pool['listen'] %>
    listen.owner = <%= @pool['listen_owner'] %>
    listen.group = <%= @pool['listen_group'] %>
    listen.mode = 0660
    pm = dynamic
    pm.max_children = 10
    pm.start_servers = 4
    pm.min_spare_servers = 2
    pm.max_spare_servers = 6
    chdir = /
    php_admin_value[error_log] = <%= @pool['error_log'] %>
    